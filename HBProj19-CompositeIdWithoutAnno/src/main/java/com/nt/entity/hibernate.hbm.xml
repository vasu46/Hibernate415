<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping>
 <class name="com.nt.entity.ProgrammerProjectInfo" table="PROGRAMMER_PROJECT_INFO">
    <composite-id name="id" class="com.nt.entity.ProgrammerProjectId">
      <key-property name="progmmerId" column="PROGRAMMER_ID"/>
      <key-property name="projectId" column="PROJECT_ID"/>
    </composite-id>
    
    <property name="projectName" column="PROJECT_NAME"/>
    <property name="programmerName" column="PROGRAMMER_NAME"/>
    <property name="salary" column="SALARY"/>
    <property name="budget" column="BUDGET"/>
    
  
 </class>
</hibernate-mapping>